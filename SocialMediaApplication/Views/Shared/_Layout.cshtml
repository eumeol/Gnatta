<!DOCTYPE html>
<html ng-app="mainApp" ng-cloak>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
</head>

<body ng-controller="mainCtrl as vm">

    @RenderBody()

    @RenderSection("scripts", false)
 
    @*Library*@
    <script src="~/Scripts/jquery-3.1.1.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-resource.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/angular-animate.js"></script>
    <script src="~/Scripts/angular-sanitize.js"></script>
    <script src="~/Scripts/ngToast.js"></script>
  

    @*Application*@
    <script src="~/apps/app.js"></script>

    @*Services*@
    <script src="~/apps/Modules/gitService.js"></script>
    <script src="~/apps/Modules/dbService.js"></script>
    <script src="~/apps/Modules/localResource.js"></script>    

    @*Controller*@
    <script src="~/apps/Controllers/mainCtrl.js"></script>
    <script src="~/apps/Controllers/UserEditCtrl.js"></script>   


</body>
</html>
